@import "../../common/lib/weui.wxss";

.container{
  background: #ffffff;
}
.page-section{
  margin-bottom: 10rpx;
}
.custom_input{
  padding-left: 10px;
  height: 30px;
  vertical-align:middle;
  background: #FFFFFF;
}
.icon_spView{
  float: left;
  margin-left: 10px;
}
.input_spView{
  margin-left: 10px;
  margin-right: 15px;
}
.custom_input_spView{
    border:  .5px solid #909090;
    border-radius: 20px;
    margin-left: 10px;
    margin-right: 10px;
    margin-top: 10px;
    height: 30px;
    background: #FFFFFF;
    
}
.text-box{
  margin-bottom: 10rpx;
  padding: 40rpx 0;
  display: flex;
  min-height: 40rpx;
  justify-content: center;
  align-items: center;
  text-align: center;
  font-size: 36rpx;
  color: #e9a740;
  line-height: 2em;
}
.btn_style{
  display: flex;
  align-items: center;
  justify-content: center;
  text-align: center;
  background:#e9a740;
  color: #333333;
  height: 70rpx;
  width: 28vw;
  /* border-radius: 16rpx; */
  font-size: 28rpx;
}
.btn_right_style{
  margin-left: 2vw;
}
